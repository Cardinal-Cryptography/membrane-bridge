    "description": "azero most bridge",
    "scripts": {
        "watch": "gulp default",
        "compile": "typechain-compiler --release --toolchain nightly-2024-02-16",
        "predrink-tests": "npm config set script-shell \"/bin/bash\"; ./scripts/predrink_tests.sh",
